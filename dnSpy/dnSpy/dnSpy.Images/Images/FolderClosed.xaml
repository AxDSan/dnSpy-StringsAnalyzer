<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-yellow-50" Color="#996f00" Opacity="0.5"/>
      <SolidColorBrush x:Key="light-yellow" Color="#996f00" Opacity="1"/>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-yellow-50}"
                               Geometry="F1 M 14.5,3.5 L 14.5,12.5 1.5,12.5 1.5,2.5 7.5,2.5 8.5,3.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-yellow}"
                               Geometry="F1 M 14.5,3 L 8.707,3 7.854,2.146 7.5,2 1.5,2 1,2.5 1,12.5 1.5,13 14.5,13 15,12.5 15,3.5 M 7.293,3 L 7.781,3.488 7.268,4 2,4 2,3 M 14,12 L 2,12 2,5 7.475,5 7.829,4.854 8.682,4 14,4 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>