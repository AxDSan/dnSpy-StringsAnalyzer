<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-blue-10" Color="#005dba" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-blue-10}" Geometry="F1 M 1.5,1.5 L 1.5,14.5 14.5,14.5 14.5,1.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-blue}"
                               Geometry="F1 M 14.5,1 L 1.5,1 1,1.5 1,14.5 1.5,15 14.5,15 15,14.5 15,1.5 M 14,14 L 2,14 2,2 14,2 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-blue}"
                               Geometry="F1 M 8,6 L 6,6 6,8 5,8 5,6 3,6 3,5 5,5 5,3 6,3 6,5 8,5 M 13,5 L 9,5 9,6 13,6 M 7.5,10 L 3.5,10 3.5,11 7.5,11 M 7.5,13 L 3.5,13 3.5,12 7.5,12 M 9,13 L 10.155,13 12.464,9 11.309,9 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>