<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 10.854,13.146 L 10.854,13.854 8.854,15.854 8.146,15.146 9.793,13.5 8.146,11.854 8.854,11.146 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-blue}" Geometry="F1 M 7,12 L 7,15 4,15 4,12 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 2.146,11.146 L 2.854,11.854 1.207,13.5 2.854,15.146 2.146,15.854 0.146,13.854 0.146,13.146 Z"/>
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                                 Geometry="F1 M 13.5,4.5 L 13.5,14.5 11.621,14.5 11.854,14.268 11.854,12.732 8.854,9.732 7.586,11 3.414,11 2.5,10.086 2.5,1.5 10.5,1.5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 14,4.5 L 14,14.5 13.5,15 11.121,15 11.854,14.268 11.854,14 13,14 13,5 10,5 10,2 3,2 3,10.586 2,9.586 2,1.5 2.5,1 10.5,1 10.854,1.146 13.854,4.146 Z"/>
              </DrawingGroup>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>