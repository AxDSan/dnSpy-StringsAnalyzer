              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 13,4.5 L 13,6 12,6 12,5 9,5 9,2 2,2 2,14 8,14 8,15 1.5,15 1,14.5 1,1.5 1.5,1 9.5,1 9.854,1.146 12.854,4.146 Z"/>
