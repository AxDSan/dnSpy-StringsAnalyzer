<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-darkblue-10" Color="#313c9e" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
      <SolidColorBrush x:Key="white" Color="#ffffff" Opacity="1"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-darkblue-10}"
                                 Geometry="F1 M 9,7 L 6.5,7 6.5,2.5 14.5,2.5 14.5,8.49 A 4.324,4.324 0 0 0 10.482,8.5 L 9,8.5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-blue}"
                                 Geometry="F1 M 12.5,5 L 8.5,5 8.5,4 12.5,4 M 8.5,7 L 12.5,7 12.5,6 8.5,6 M 10,8.762 A 4.474,4.474 0 0 1 12.5,8 L 10,8 M 14.5,2 L 6.5,2 6,2.5 6,7 7,7 7,3 14,3 14,8 12.5,8 A 4.448,4.448 0 0 1 14.83,8.67 L 15,8.5 15,2.5 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-blue}"
                               Geometry="F1 M 8.276,14 L 1.5,14 1,13.5 1,7.5 1.5,7 9.5,7 10,7.5 10,8.762 A 4.5,4.5 0 0 0 8,12.5 4.445,4.445 0 0 0 8.276,14 Z"/>
              <GeometryDrawing Brush="{DynamicResource white}" Geometry="F1 M 8,11 L 3,11 3,10 8,10 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 12.5,9.5 A 3,3 0 1 1 9.5,12.5 3,3 0 0 1 12.5,9.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 9,12.5 A 3.5,3.5 0 1 0 12.5,9 3.5,3.5 0 0 0 9,12.5 M 15,12.5 A 2.486,2.486 0 0 1 14.584,13.877 L 11.123,10.416 A 2.5,2.5 0 0 1 15,12.5 M 10,12.5 A 2.486,2.486 0 0 1 10.416,11.123 L 13.877,14.584 A 2.5,2.5 0 0 1 10,12.5 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>