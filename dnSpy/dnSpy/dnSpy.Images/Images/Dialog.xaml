<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                                 Geometry="F1 M 14.5,4.5 L 14.5,13.5 1.5,13.5 1.5,4.5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 14.5,4 L 1.5,4 1,4.5 1,13.5 1.5,14 14.5,14 15,13.5 15,4.5 M 14,13 L 2,13 2,5 14,5 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}" Geometry="F1 M 6,11 L 9,11 9,12 6,12 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}" Geometry="F1 M 10,12 L 13,12 13,11 10,11 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}" Geometry="F1 M 7,6 L 3,6 3,7 7,7 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}" Geometry="F1 M 7,8 L 3,8 3,9 7,9 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 14.5,2.5 L 14.5,4.5 1.5,4.5 1.5,2.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 12.5,8.5 L 8.5,8.5 8.5,6.5 12.5,6.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 14.5,5 L 1.5,5 1,4.5 1,2.5 1.5,2 14.5,2 15,2.5 15,4.5 M 2,4 L 14,4 14,3 2,3 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 12.5,6 L 8.5,6 8,6.5 8,8.5 8.5,9 12.5,9 13,8.5 13,6.5 M 12,8 L 9,8 9,7 12,7 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>