<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 8,11.5 L 7,11.5 2.5,11.5 2.5,3.5 11.5,3.5 11.5,8.122 A 4.5,4.5 0 0 0 8.121,11.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 8.051,12 L 2.5,12 2,11.5 2,3.5 2.5,3 11.5,3 12,3.5 12,8.051 A 4.485,4.485 0 0 0 11,8.276 L 11,4 3,4 3,11 8.276,11 A 4.485,4.485 0 0 0 8.051,12 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-blue}" Geometry="F1 M 9,7 L 5,7 5,6 9,6 M 9,8 L 5,8 5,9 9,9 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 12.5,9.5 A 3,3 0 1 1 9.5,12.5 3,3 0 0 1 12.5,9.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 9,12.5 A 3.5,3.5 0 1 0 12.5,9 3.5,3.5 0 0 0 9,12.5 M 15,12.5 A 2.486,2.486 0 0 1 14.584,13.877 L 11.123,10.416 A 2.5,2.5 0 0 1 15,12.5 M 10,12.5 A 2.486,2.486 0 0 1 10.416,11.123 L 13.877,14.584 A 2.5,2.5 0 0 1 10,12.5 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>