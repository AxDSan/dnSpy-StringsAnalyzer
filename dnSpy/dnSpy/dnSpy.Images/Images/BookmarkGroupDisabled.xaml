<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <System:Double x:Key="cls-1">0</System:Double>
      <System:Double x:Key="cls-2">0.5</System:Double>
      <System:Double x:Key="cls-3">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas" Opacity="{DynamicResource cls-1}">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 6.647,10.647 L 6.353,10.647 2.5,14.5 2.5,3.5 10.5,3.5 10.5,14.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 10.5,3 L 2.5,3 2,3.5 2,14.5 2.854,14.854 6.5,11.208 10.146,14.854 11,14.5 11,3.5 M 10,13.293 L 6.853,10.15 6.147,10.15 3,13.293 3,4 10,4 Z"/>
              <DrawingGroup Opacity="{DynamicResource cls-2}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 3,6.293 L 6.854,10.146 6.854,10.146 6.148,10.146 5.794,10.496 3,7.707 M 10,10.707 L 10,9.293 4.707,4 3.293,4 M 8.707,4 L 7.293,4 10,6.707 10,5.293 M 4.5,11.793 L 3,10.293 3,11.707 3.793,12.5 Z"/>
              </DrawingGroup>
              <DrawingGroup Opacity="{DynamicResource cls-3}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 13.5,1 L 8.858,1 8.858,1 5.5,1 5,1.5 5,3 6,3 6,2 13,2 13,11.464 11,9.477 10.987,10.877 13.147,13.028 14,12.671 14,1.5 Z"/>
                <DrawingGroup Opacity="{DynamicResource cls-2}">
                  <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                                   Geometry="F1 M 13.5,1.5 L 13.5,12.667 11,10.178 11,3.5 10.5,3 5.5,3 5.5,1.5 Z"/>
                </DrawingGroup>
              </DrawingGroup>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>