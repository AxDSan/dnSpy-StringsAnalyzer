<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-purple-10" Color="#6936aa" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-purple" Color="#6936aa" Opacity="1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-purple-10}" Geometry="F1 M 13.5,9.5 L 13.5,14.5 1.5,14.5 1.5,9.5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-purple}"
                                 Geometry="F1 M 13.5,9 L 1.5,9 1,9.5 1,14.5 1.5,15 13.5,15 14,14.5 14,9.5 M 13,14 L 2,14 2,10 13,10 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-purple-10}"
                               Geometry="F1 M 1.5,2.5 L 6.5,2.5 6.5,7.5 1.5,7.5 M 8.5,2.5 L 8.5,7.5 13.5,7.5 13.5,2.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-purple}"
                               Geometry="F1 M 1.5,2 L 1,2.5 1,7.5 1.5,8 6.5,8 7,7.5 7,2.5 6.5,2 M 6,7 L 2,7 2,3 6,3 M 13.5,2 L 8.5,2 8,2.5 8,7.5 8.5,8 13.5,8 14,7.5 14,2.5 M 13,7 L 9,7 9,3 13,3 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>