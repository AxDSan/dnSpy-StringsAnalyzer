<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <SolidColorBrush x:Key="light-green" Color="#1f801f" Opacity="1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}" Geometry="F1 M 14.5,5 L 14.5,13 1.5,13 1.5,5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 14.5,5 L 1.5,5 1,5.5 1,13.5 1.5,14 14.5,14 15,13.5 15,5.5 M 14,13 L 2,13 2,6 14,6 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-green}"
                               Geometry="F1 M 8.5,11 L 12,11 12,12 8.5,12 M 4,12 L 7.5,12 7.5,11 4,11 M 10,10 L 12,10 12,9 10,9 M 4,10 L 9,10 9,9 4,9 M 4,8 L 9,8 9,7 4,7 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}" Geometry="F1 M 14,3 L 14,5 2,5 2,3 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 14.5,6 L 1.5,6 1,5.5 1,2.5 1.5,2 14.5,2 15,2.5 15,5.5 M 2,5 L 14,5 14,3 2,3 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>