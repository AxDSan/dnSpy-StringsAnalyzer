<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <SolidColorBrush x:Key="white" Color="#ffffff" Opacity="1"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey-25" Color="#212121" Opacity="0.25"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 7,0.5 L 7,6.5 6.5,7 0.5,7 0,6.5 0,0.5 0.5,0 6.5,0 Z"/>
              <GeometryDrawing Brush="{DynamicResource white}"
                               Geometry="F1 M 5.146,5.854 L 3.5,4.207 2,5.707 1.293,5 2.793,3.5 1.293,2 2,1.293 3.5,2.793 5,1.293 5.707,2 4.207,3.5 5.854,5.146 Z"/>
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                                 Geometry="F1 M 8.5,2.5 L 8.5,6 12.5,6 13,6.5 13,11.5 15.5,11.5 15.5,2.5 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}" Geometry="F1 M 2.5,8 L 2.5,13.5 5,13.5 5,8 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 15.5,2 L 8.5,2 8,2.5 8,6 9,6 9,3 15,3 15,11 13,11 13,12 15.5,12 16,11.5 16,2.5 M 3,8 L 2,8 2,13.5 2.5,14 5,14 5,13 3,13 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 8,6.5 L 8,6.91 6.91,8 5.5,8 5.5,15.5 12.5,15.5 12.5,6.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 12.5,6 L 8,6 8,6.91 7.91,7 12,7 12,15 6,15 6,8 5,8 5,15.5 5.5,16 12.5,16 13,15.5 13,6.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-25}"
                               Geometry="F1 M 9,11 L 9,7 8,7 8,11.5 8.5,12 12,12 12,11 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>