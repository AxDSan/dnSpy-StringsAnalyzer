<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 14,4 L 14,13.5 13.5,14 1.5,14 1,13.5 1,4 1.5,4 1.5,2.5 13.5,2.5 13.5,4 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 2,8 L 1,8 1,7 2,7 M 14,8 L 13,8 13,7 14,7 M 2,10 L 1,10 1,9 2,9 M 14,10 L 13,10 13,9 14,9 M 2,12 L 1,12 1,11 2,11 M 14,12 L 13,12 13,11 14,11 M 4,14 L 3,14 3,13 4,13 M 6,14 L 5,14 5,13 6,13 M 8,14 L 7,14 7,13 8,13 M 10,14 L 9,14 9,13 10,13 M 12,14 L 11,14 11,13 12,13 M 2,14 L 1.5,14 1,13.5 1,13 2,13 M 13,13 L 14,13 14,13.5 13.5,14 13,14 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 13.5,2 L 1.5,2 1,2.5 1,6 2,6 2,5 13,5 13,6 14,6 14,2.5 M 13,4 L 2,4 2,3 13,3 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>