<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-yellow-10" Color="#996f00" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-yellow" Color="#996f00" Opacity="1"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <System:Double x:Key="cls-1">0</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas" Opacity="{DynamicResource cls-1}">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 9.5,7.5 L 15.5,7.5 15.5,15 12.5,12 9.5,15 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-yellow-10}"
                               Geometry="F1 M 8,13.505 L 0.5,13.505 0.5,3.5 5.5,3.5 6.5,4.5 13.5,4.5 13.5,6 9.086,6 8,7.086 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-yellow}"
                               Geometry="F1 M 8,13 L 1,13 1,6 5.475,6 5.829,5.854 6.682,5 13,5 13,6 14,6 14,4.5 13.5,4 6.707,4 5.854,3.146 5.5,3 0.5,3 0,3.5 0,13.5 0.5,14 8,14 M 1,4 L 5.293,4 5.781,4.488 5.268,5 1,5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-blue}"
                               Geometry="F1 M 3,9 L 7.293,9 5.9,7.6 6.6,6.892 8.85,9.142 8.85,9.849 6.6,12.1 5.9,11.392 7.3,9.992 3,10 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 15.5,7 L 9.5,7 9,7.5 9,15 9.854,15.354 12.5,12.707 15.146,15.354 16,15 16,7.5 M 15,13.793 L 12.854,11.646 12.146,11.646 10,13.793 10,8 15,8 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>