<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-yellow-10" Color="#996f00" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-yellow" Color="#996f00" Opacity="1"/>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <GeometryDrawing Brush="{DynamicResource light-yellow-10}"
                               Geometry="F1 M 12.5,6.5 L 4.5,14.5 3,14.5 6.5,8.5 3,8.5 7,1.5 12.387,1.5 8,6.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-yellow}"
                               Geometry="F1 M 12.5,6 L 9.1,6 12.759,1.83 12.387,1 7,1 6.566,1.252 2.566,8.252 3,9 5.629,9 2.568,14.248 3,15 4.5,15 4.854,14.854 12.854,6.854 M 4.293,14 L 3.871,14 6.932,8.752 6.5,8 3.862,8 7.29,2 11.283,2 7.624,6.17 8,7 11.293,7 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>