<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
      <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
      <SolidColorBrush x:Key="light-purple" Color="#6936aa" Opacity="1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                                 Geometry="F1 M 4,14 L 4,13.5 1.5,13.5 1.5,4.5 15,4.5 15,6.936 13,6 9.191,9.661 7,8 6,8.273 6,12.727 7,13 5.75,14 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 14,5.364 L 14,5 14.5,5 15,4.5 15,5.832 Z"/>
                <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                                 Geometry="F1 M 5,14 L 1.5,14 1,13.5 1,4.5 1.5,5 2,5 2,13 5,13 5,14 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey-10}"
                               Geometry="F1 M 14.5,1.5 L 14.5,4.5 1.5,4.5 1.5,1.5 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 14.5,1 L 1.5,1 1,1.5 1,4.5 1.5,5 14.5,5 15,4.5 15,1.5 M 14,4 L 2,4 2,2 14,2 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-purple}"
                               Geometry="F1 M 13,6 L 9.191,9.661 7,8 6,8.273 6,12.727 7,13 9.191,11.339 13,15 15,14.064 15,6.936 M 7,11.766 L 7,9.234 8.317,10.5 M 13,12.548 L 10.3,10.5 13,8.452 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>