              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 15,4 L 15,5 7.947,5 A 3.936,3.936 0 0 0 8.359,4 M 7,9 L 15,9 15,8 7,8 M 7,13 L 15,13 15,12 7,12 Z"/>
              <GeometryDrawing Brush="{DynamicResource light-defaultgrey}"
                               Geometry="F1 M 2,14 L 5,14 5,11 2,11 M 3,12 L 4,12 4,13 3,13 M 5,8.156 L 5,10 3.157,10 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>