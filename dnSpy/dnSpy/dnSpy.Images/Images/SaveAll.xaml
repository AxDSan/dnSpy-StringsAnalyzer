<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:System="clr-namespace:System;assembly=mscorlib"
         Width="16 " Height="16">
  <Rectangle Width="16 " Height="16">
    <FrameworkElement.Resources>
      <SolidColorBrush x:Key="canvas" Opacity="0"/>
      <SolidColorBrush x:Key="light-blue" Color="#005dba" Opacity="1"/>
      <SolidColorBrush x:Key="light-blue-10" Color="#005dba" Opacity="0.1"/>
      <System:Double x:Key="cls-1">0.75</System:Double>
    </FrameworkElement.Resources>
    <Shape.Fill>
      <DrawingBrush Stretch="None">
        <DrawingBrush.Drawing>
          <DrawingGroup>
            <DrawingGroup x:Name="canvas">
              <GeometryDrawing Brush="{DynamicResource canvas}" Geometry="F1 M 16,16 L 0,16 0,0 16,0 Z"/>
            </DrawingGroup>
            <DrawingGroup x:Name="level_1">
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-blue}"
                                 Geometry="F1 M 14.85,2.65 L 13.35,1.15 13,1 3.5,1 3,1.5 3,2 12.79,2 14,3.21 14,13 14.49,13 15,12.5 15,3 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-blue-10}"
                               Geometry="F1 M 12.515,5 L 12.515,14.5 1.515,14.5 1.515,3.5 11.015,3.5 Z"/>
              <DrawingGroup Opacity="{DynamicResource cls-1}">
                <GeometryDrawing Brush="{DynamicResource light-blue}"
                                 Geometry="F1 M 11,9.5 L 11,14 10,14 10,10 4,10 4,14 3,14 3,9.5 3.5,9 10.5,9 Z"/>
              </DrawingGroup>
              <GeometryDrawing Brush="{DynamicResource light-blue}"
                               Geometry="F1 M 12.854,4.647 L 11.354,3.147 11,3 1.5,3 1,3.5 1,14.517 1.485,15 12.485,15 13,14.5 13,5 M 5,4 L 9,4 9,7 5,7 M 12,14 L 2,14 2,4 4,4 4,7.5 4.5,8 9.5,8 10,7.5 10,4 10.793,4 12,5.208 Z"/>
            </DrawingGroup>
          </DrawingGroup>
        </DrawingBrush.Drawing>
      </DrawingBrush>
    </Shape.Fill>
  </Rectangle>
</Viewbox>